
public class StringLoopsQuizFix {

	public static void main(String[] args) {
		
		System.out.println(giveBackwards("abc123"));
		
	}

	
	public static String giveBackwards(String str) {
		
		String result = "";
		
		for(int i = 1; i < str.length()+1; i++) {
			char theChars = str.charAt(str.length()-i);
			result += theChars;
			
		}
		return result;
	}
	
}
